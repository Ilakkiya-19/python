# Initialize an empty string to hold the word without vowels
word_without_vowels = ""

# Prompt the user to enter a word and assign it to the user_word variable
user_word = input("Enter a word: ")

# Define a set of vowels (both lowercase and uppercase)
vowels = "aeiouAEIOU"
for letter in user_word:
    # If the letter is not a vowel, add it to word_without_vowels
    if letter not in vowels:
        word_without_vowels += letter

# Print the word assigned to word_without_vowels
print(word_without_vowels)
